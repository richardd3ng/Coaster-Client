{
    "expo": {
        "name": "Coaster",
        "slug": "Coaster",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#C5FF95"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.Coaster",
            "infoPlist": {
                "LSApplicationQueriesSchemes": ["spotify"]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.Coaster"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "b416cf5c-3b9e-45e2-ad40-dab80f550344"
            }
        },
        "scheme": "Coaster",
        "plugins": [
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow Coaster to use your location."
                }
            ]
        ]
    }
}
